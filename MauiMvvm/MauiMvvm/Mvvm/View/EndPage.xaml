<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiMvvm.Mvvm.View.EndPage"
             Title="EndPage">
    <VerticalStackLayout>
        <Label 
     Text="{Binding Nev.Vezeteknev}"
     FontSize="Large"
     VerticalOptions="Center" 
     HorizontalOptions="Center" />
        <Label 
     Text="{Binding Nev.Keresztnev}"
     FontSize="Large"
     VerticalOptions="Center" 
     HorizontalOptions="Center" />
        <Button x:Name="buttonVissza" Text="Vissza" Clicked="buttonVissza_Clicked" />
        <Button x:Name="buttonVisszaStart" Text="Start" Clicked="buttonVisszaStart_Clicked" />
    </VerticalStackLayout>
</ContentPage>